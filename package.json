{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "null-pointer-upload",
  "title": "Null Pointer Upload",
  "description": "Upload files to http://0x0.st",
  "icon": "extension_icon.png",
  "author": "Joehoel",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "upload",
      "title": "Upload",
      "description": "Upload a file to http://0x0.st",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.47.3",
    "@raycast/utils": "^1.4.16"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
